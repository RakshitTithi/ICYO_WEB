   "source": [
    "#Progress bar while loading the image\n",
    "if st.checkbox('Show me the image'):\n",
    "    import time\n",
    "\n",
    "    'Loading the image...'\n",
    "\n",
    "    # Add a placeholder\n",
    "    latest_iteration = st.empty()\n",
    "    bar = st.progress(0)\n",
    "\n",
    "    for i in range(100):\n",
    "        # Update the progress bar with each iteration.\n",
    "        latest_iteration.text(f'Iteration {i+1}')\n",
    "        bar.progress(i + 1)\n",
    "        time.sleep(0.1)\n",
    "\n",
    "    '...image loaded'"
   ]
